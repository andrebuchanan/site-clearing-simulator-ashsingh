{
  "name": "clearing-site",
  "version": "0.1.0",
  "private": true,
  "author": "Ashwini Singh",
  "scripts": {
    "build": "react-scripts build",
    "lint": "eslint \"./**/*.{js,jsx,ts,tsx}\" --fix",
    "start": "react-scripts start",
    "test": "react-scripts test --coverage"
  },
  "husky": {
